{
  "name": "server",
  "version": "1.0.0",
  "description": "ThriveHive Microservice",
  "main": "index.js",
  "scripts": {
    "admin": "cd admin && npm start",
    "build": "yarn build-pre && webpack --colors --progress --config \"./config/webpack/webpack.js\"",
    "build-dev": "yarn build-pre && webpack --colors --progress --config \"./config/webpack/webpack.dev.js\"",
    "build-pre": "yarn install && yarn run clean",
    "build-watch": "yarn build-pre && webpack --colors --progress --config \"./config/webpack/webpack.dev.js\" --watch",
    "clean": "rm -rf dist/",
    "selenium": "echo 'Yay selenium!'",
    "start": "pm2 startOrRestart process.json",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test node --inspect ./node_modules/.bin/mocha-webpack",
    "test:coverage": "yarn cross-env NODE_ENV=test yarn nyc yarn mocha-webpack",
    "test:windows": "yarn cross-env NODE_ENV=test yarn mocha-webpack",
    "watch": "concurrently --kill-other --raw \"yarn build-watch\" \"pm2 startOrRestart process.dev.json\"",
    "watch:log": "concurrently --kill-other --raw \"yarn build-watch\" \"pm2 logs server --lines 0 startOrRestart process.dev.json\"",
    "build:watch": "yarn build && yarn watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/propelmarketing/<server>.git"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "author": "dev@thrivehive.com",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/propelmarketing/<server>/issues"
  },
  "homepage": "https://github.com/propelmarketing/<server>#readme",
