            <tr><td></td></tr>
            <tr><td></td></tr>
            <tr><td></td></tr>
            <tr>
              <td>
                <a href="//my.thrivehive.com" target="_blank">
                  <object
                    width="20%"
                    height="auto"
                    data="<%= cdn %>/<%= agency %>/img/png/logo.png"
                    type="image/png"
                  ></object>
                </a>
              </td>
            </tr>
          </table>
        </td>
        <td></td>
      </tr>
