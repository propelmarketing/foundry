{
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types",
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "alias": {
          "arbiter": "./src/arbiter",
          "thrivehive": "./src/thrivehive",
          "static": "./static",
          "configuration": "./config",
          "test": "./test"
        }
      }
    ]
  ],
  "presets": [
    [
      "env",
      {
        "loose": true,
        "useBuiltIns": true
      }
    ],
    "stage-0",
  ],
  "sourceMaps": true,
  "ignore": [
    "node_modules"
  ]
}
